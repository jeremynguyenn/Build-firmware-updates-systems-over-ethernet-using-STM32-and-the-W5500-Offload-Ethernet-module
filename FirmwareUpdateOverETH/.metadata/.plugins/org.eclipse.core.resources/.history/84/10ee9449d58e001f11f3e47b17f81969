 80003fe:	b08b      	sub	sp, #44	@ 0x2c
 8000400:	af00      	add	r7, sp, #0
 8000402:	60f8      	str	r0, [r7, #12]
 8000404:	60b9      	str	r1, [r7, #8]
 8000406:	607a      	str	r2, [r7, #4]
 8000408:	807b      	strh	r3, [r7, #2]
	uint16_t slen;		/* Length of current segment */
